@model List<VacaYAY.Models.ReturnRequestViewModel>
@{
}

<!DOCTYPE html>
@Html.Hidden("pageCount", 1);
@Html.Hidden("pageOffset", 0);
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>VacaYAY</title>
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")
    @Styles.Render("~/bundles/modernizr")
    <script src="~/Scripts/plugins/perfect-scrollbar.jquery.min.js"></script>
</head>
<body class="mt-3">
    <div class="row">
        <div class="col-md-3">Ovde idu neki podaci o adminu</div>
        <div class="col-md-6">
            Kartice za requestove
            <div class="panel-group" id="accordion">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a data-toggle="collapse" data-parent="#accordion" href="#collapse1">
                                Collapsible Group 1
                            </a>
                        </h4>
                    </div>
                    <div id="collapse1" class="panel-collapse collapse in">
                        <div class="panel-body">
                            Lorem ipsum dolor sit amet, consectetur adipisicing elit,
                            sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad
                            minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea
                            commodo consequat.
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-3">Podacic neki drugi</div>
    </div>
</body>
<script>
    $(document).ready(function()
    {
        $("#pageOffset").val($(window).height() / $(".panel").height());

        document.cookie = "pageOffset=" + $("#pageOffset").val();
        document.cookie = "pageCount=" + $("#pageCount").val();

        console.log("uradio sam ");
        GetRequests();
        console.log($("#pageOffset").val());
    });

    function GetRequests()
    {
         @()
         $("#pageCount").val($("#pageCount") + 1)
    }
</script>
</html>
